<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="board">

    <select id="selectBoard" resultType="kr.go.spo.model.BoardPro">
        SELECT
            A.bno
             , A.subject
             , A.content
        FROM board_pro A
        WHERE 1=1
            limit 10
    </select>

</mapper>
