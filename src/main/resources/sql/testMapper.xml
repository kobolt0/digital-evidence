<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="test">

    <select id="selectTest" resultType="java.util.Map">
        SELECT
            A.id_
             , A.rev_
             , A.name_
             , A.type_
        FROM ACT_ID_GROUP A
        WHERE 1=1
    </select>

</mapper>
